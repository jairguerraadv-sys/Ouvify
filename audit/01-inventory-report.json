  "database_models": {
    "Client (tenants.Client)": {
      "description": "Modelo de Tenant (empresa cliente)",
      "fields": [
        "id (PK)",
        "owner (FK User)",
        "nome",
        "subdominio (unique)",
        "logo (URL Cloudinary)",
        "cor_primaria",
        "cor_secundaria",
        "cor_texto",
        "fonte_customizada",
        "favicon (URL Cloudinary)",
        "ativo (boolean)",
        "plano (choices: free/starter/pro)",
        "stripe_customer_id (unique)",
        "stripe_subscription_id",
        "subscription_status",
        "data_fim_assinatura",
        "data_criacao",
        "data_atualizacao"
      ],
      "indexes": [
        "subdominio (unique)",
        "stripe_customer_id (unique)"
      ],
      "relationships": [
        "OneToMany → Feedback",
        "OneToMany → FeedbackInteracao",
        "OneToMany → FeedbackArquivo",
        "OneToMany → CSPViolation"
      ]
    },
    
    "Feedback (feedbacks.Feedback)": {
      "description": "Feedback de usuário (TenantAwareModel)",
      "fields": [
        "id (PK)",
        "client (FK Client) - isolamento multi-tenant",
        "tipo (choices: denuncia/sugestao/elogio/reclamacao)",
        "titulo",
        "descricao",
        "status (choices: pendente/em_analise/resolvido/fechado)",
        "protocolo (unique, indexed) - gerado automaticamente",
        "anonimo (boolean)",
        "email_contato",
        "resposta_empresa",
        "data_resposta",
        "data_criacao",
        "data_atualizacao",
        "autor (FK User, nullable)"
      ],
      "indexes": [
        "protocolo (unique)",
        "(client, tipo)",
        "(client, status)",
        "(client, -data_criacao)",
        "(client, status, -data_criacao)"
      ],
      "relationships": [
        "ManyToOne → Client",
        "OneToMany → FeedbackInteracao",
        "OneToMany → FeedbackArquivo"
      ],
      "security": "✅ Protocolo usa secrets.choice() (CSPRNG) - correção 2026-01-27"
    },
    
    "FeedbackInteracao (feedbacks.FeedbackInteracao)": {
      "description": "Interações/comentários em feedbacks (TenantAwareModel)",
      "fields": [
        "id (PK)",
        "client (FK Client)",
        "feedback (FK Feedback)",
        "autor (FK User, nullable)",
        "tipo (choices: MENSAGEM_PUBLICA/PERGUNTA_EMPRESA/RESPOSTA_USUARIO/MUDANCA_STATUS/NOTA_INTERNA/MENSAGEM_AUTOMATICA)",
        "mensagem",
        "data"
      ],
      "indexes": [
        "(client, feedback, data)",
        "(feedback, tipo)"
      ],
      "relationships": [
        "ManyToOne → Client",
        "ManyToOne → Feedback",
        "ManyToOne → User"
      ],
      "feature_gating": "NOTA_INTERNA requer allow_internal_notes"
    },
    
    "FeedbackArquivo (feedbacks.FeedbackArquivo)": {
      "description": "Arquivos anexados a feedbacks (TenantAwareModel)",
      "fields": [
        "id (PK)",
        "client (FK Client)",
        "feedback (FK Feedback)",
        "arquivo (CloudinaryField)",
        "nome_original",
        "tipo_mime",
        "tamanho_bytes",
        "enviado_por (FK User, nullable)",
        "interno (boolean)",
        "data_upload"
      ],
      "indexes": [
        "(client, feedback)",
        "(feedback, interno)"
      ],
      "relationships": [
        "ManyToOne → Client",
        "ManyToOne → Feedback",
        "ManyToOne → User"
      ],
      "feature_gating": "allow_attachments (plano PRO)",
      "limits": "10MB máximo, tipos: imagens/PDF/Office"
    },
    
    "CSPViolation (core.CSPViolation)": {
      "description": "Registros de violação CSP para monitoramento",
      "fields": [
        "id (PK)",
        "client (FK Client)",
        "document_uri",
        "violated_directive",
        "effective_directive",
        "original_policy",
        "blocked_uri",
        "source_file",
        "line_number",
        "user_agent",
        "ip_address",
        "created_at"
      ],
      "indexes": [
        "(client, created_at)",
        "violated_directive",
        "ip_address"
      ],
      "security": "URIs sanitizadas (sem query params), rate limited"
